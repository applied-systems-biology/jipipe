        <module>jipipe-clij</module>
        <module>jipipe-strings</module>
        <module>jipipe-utils</module>
        <module>jipipe-forms</module>
        <module>jipipe-r</module>
        <module>jipipe-cellpose</module>
        <module>jipipe-deep-learning</module>
        <module>jipipe-launcher</module>
        <module>ij-updater-cli</module>
        <module>jipipe-installer-linux</module>
