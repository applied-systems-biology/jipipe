    <dependencies>
        <dependency>
            <groupId>org.junit.jupiter</groupId>
            <artifactId>junit-jupiter</artifactId>
            <scope>test</scope>
        </dependency>
    </dependencies>

    <properties>
        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
        <license.licenseName>BSD-2-Clause</license.licenseName>
        <license.copyrightOwners>N/A</license.copyrightOwners>
        <license.projectName>JIPipe</license.projectName>
    </properties>

    <modules>
        <module>jipipe-core</module>
        <module>jipipe-plots</module>
        <module>jipipe-python</module>
        <module>jipipe-ij</module>
        <module>jipipe-ij-omero</module>
        <module>jipipe-ij-algorithms</module>
        <module>jipipe-ij-multi-template-matching</module>
        <module>jipipe-annotation</module>
        <module>jipipe-filesystem</module>
        <module>jipipe-tables</module>
