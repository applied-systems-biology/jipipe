    <properties>
        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
        <license.licenseName>mit</license.licenseName>
        <license.copyrightOwners>N/A</license.copyrightOwners>
        <license.projectName>JIPipe</license.projectName>
        <releaseProfiles>sign,deploy-to-scijava</releaseProfiles>

        <!-- Versions -->
        <jipipe.version>${project.version}</jipipe.version>
        <vtk.version>9.2.6</vtk.version>
    </properties>

    <profiles>
        <profile>
            <id>Unix</id>
            <activation>
                <os>
                    <family>unix</family>
                </os>
            </activation>
            <properties>
                <vtk.platform>linux_x86_64</vtk.platform>
            </properties>
        </profile>
        <profile>
            <id>MacOS x86_64</id>
            <activation>
                <os>
                    <family>mac</family>
                    <arch>x86_64</arch>
                </os>
            </activation>

            <properties>
                <vtk.platform>macos_x86_64</vtk.platform>
            </properties>
        </profile>
        <profile>
            <id>MacOS Aarch64</id>
            <activation>
                <os>
                    <family>mac</family>
                    <arch>aarch64</arch>
                </os>
            </activation>
            <properties>
                <vtk.platform>macos_aarch64</vtk.platform>
            </properties>
        </profile>
        <profile>
            <id>Windows</id>
            <activation>
                <os>
                    <family>Windows</family>
                </os>
            </activation>
            <properties>
                <vtk.platform>windows_x86_64</vtk.platform>
            </properties>
        </profile>
    </profiles>

